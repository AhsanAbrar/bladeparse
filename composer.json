{
    "name": "ahsanabrar/bladeparse",
    "description": "parsedown laravel blade package.",
    "keywords": [
        "parsedown",
        "markdown",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahsan Abrar",
            "email": "iahsanabrar@gmail.com",
            "homepage": "http://ahsan.tech"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "erusev/parsedown-extra": "^0.7.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ahsan\\BladeParse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahsan\\BladeParse\\BladeParseServiceProvider"
            ]
        }
    }
}
